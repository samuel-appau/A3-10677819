#include<iostream>
using namespace std;

int main(){
char grade;
int score;
cout<<"Welcome to UG Grading System"<<endl;

cout<<"Please Enter your score"<<endl;

cin>>score;

switch(score)
{

      case 80 ... 100: 
cout<<"Your grade is A"<<endl;
     break;

      case 75 ... 79:
cout<<"Your grade is B+"<,endl;
     break;
      case 70 ... 74:
cout<<"Your grade is B"<<endl;
     break;
      case 65 ... 69:
cout<<"Your grade is C+"<<endl;
     break;
      case 55 ... 64 :
cout<<"Your grade is C"<<endl;
     break;
      case 45 ... 54:
cout<<"Your grade is D+"<<endl;
     break;
      case 40 ... 44:
cout<<"Your grade is D"<<endl;
     break;
      case 35 ... 39:
cout<<"Your grade is E"<<endl;
     break;
      default:
cout<<"Your grade is F"<<endl;
return 0;
}

